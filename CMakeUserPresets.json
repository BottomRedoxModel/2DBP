{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "debug",
      "displayName": "Debug Config",
      "description": "Debug with a default generator",
      "binaryDir": "${sourceDir}/build/debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "FABM_BASE": "~/src/fabm-omp",
        "FABM_NIVA_BASE": "~/src/BROM"
      }
    },
    {
      "name": "release",
      "displayName": "Release Config",
      "description": "Release with a default generator",
      "binaryDir": "${sourceDir}/build/release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "FABM_BASE": "~/src/fabm-omp",
        "FABM_NIVA_BASE": "~/src/BROM"
      }
    }
  ]
}
